{
  "name": "rehype-graphviz",
  "version": "0.2.2",
  "description": "Generate SVG from Graphviz DOT code",
  "keywords": ["rehype", "rehype-plugin", "remark", "unified", "graphviz"],
  "author": "rai",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r4ai/rehype-graphviz.git"
  },
  "bugs": {
    "url": "https://github.com/r4ai/rehype-graphviz/issues"
  },
  "homepage": "https://github.com/r4ai/rehype-graphviz/",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist", "LICENSE", "README.md"],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "vitest",
    "build": "rimraf -rf dist && tsc --project tsconfig.build.json",
    "prepublishOnly": "bun run build",
    "check": "bunx @biomejs/biome check --apply .",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "rehype": "^13.0.1",
    "unified": "^11.0.4",
    "@hpcc-js/wasm": "^2.14.1"
  },
  "dependencies": {
    "unist-util-visit": "^5.0.0",
    "hast-util-from-html-isomorphic": "^2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.2",
    "@qnighy/dedent": "^0.1.1",
    "@tsconfig/node16": "^16.1.3",
    "@types/bun": "^1.1.0",
    "@types/jsdom": "^21.1.4",
    "bun-plugin-dts": "^0.2.1",
    "bun-types": "^1.0.7",
    "hast": "^1.0.0",
    "husky": "^9.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.0.2",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
